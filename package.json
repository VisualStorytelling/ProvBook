{
  "name": "@labrastory/provbook",
  "version": "0.0.1",
  "description": "JupyterLab Notebook Provenance",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension"
  ],
  "homepage": "https://github.com/VisualStorytelling/ProvBook",
  "license": "Apache 2.0",
  "authors": [
    "Netherlands eScience Center",
    "The Caleydo Team",
    "Sheeba Samuel"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/VisualStorytelling/ProvBook"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "install-ext": "jupyter labextension install . --no-build",
    "prepare": "yarn run clean && yarn run build",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "ProvBook"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^2.2.2",
    "@jupyterlab/apputils": "^2.2.2",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/docregistry": "^2.2.1",
    "@jupyterlab/notebook": "^2.2.2",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/ui-components": "^2.2.1",
    "@lumino/disposable": "^1.4.3",
    "jquery": "^3.5.1",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslint": "5.11.0",
    "typescript": "^3.9.7"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
